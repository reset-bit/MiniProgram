.containor{
  width: 100%;
  height: 100%;
}

.input-bar{
  width: 100%;
  height: 80rpx;
  margin: 20rpx;
  display: flex;
}

.tip{
  width: 170rpx;
  line-height: 80rpx;
  font-size: 40rpx;
}

.input{
  width: 70%;
  height: 80rpx;
  border: 1rpx solid black;
  border-radius: 10rpx;
  font-size: 36rpx;
}

.suggestion-box{
  margin: 10px 25px;
  margin-left: 190rpx;
  border-bottom:1px solid #c3c3c3;
  padding-bottom:10px
}

.list{
  width: 100%;
  height: 85rpx;
  display: flex;
  justify-items: space-around;
}

.list-item{
  width: 23%;
  font-size: 38rpx;
  text-align: center;
  border-bottom: 1rpx solid gray;
  margin: 10rpx
}

#walking{
  border-bottom: 1rpx solid;
  border-bottom-color: black;
}

map{
  width: 100%;
  height: 700rpx;
  margin-top: 10rpx;
}

button{
  margin-top: 30rpx;
}